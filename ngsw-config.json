{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/icons/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "deezer-api",
      "urls": [
        "/api/deezer/**"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "1d",
        "strategy": "freshness"
      }
    },
    {
      "name": "deezer-images",
      "urls": [
        "https://e-cdns-images.dzcdn.net/**",
        "https://cdn-images.dzcdn.net/**"
      ],
      "cacheConfig": {
        "maxSize": 200,
        "maxAge": "30d",
        "strategy": "performance",
        "timeout": "10s"
      }
    }
  ]
}
